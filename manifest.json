{
  "manifest_version": 2,
  "name": "YouTabMan",
  "version": "1.2.1",

  "description": "A Web Extension used to control all of your YouTube tabs.",

  "browser_action": {
    "default_title": "YouTabMan",
    "default_popup": "popup/index.html",
    "default_icon": {
      "48": "icons/popup_icon/icon_48.png"
    }
  },

  "permissions": [
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
